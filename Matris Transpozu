#include <iostream>
using namespace std;
int main()
{
	
	int A[3][3] = { {2,3,5}, {8,9,7}, {6,4,1} };
	
	for (int i = 0; i < 3; i++) {
		for (int j = 0; j < 3; j++) {
		cout << " " << A[i][j];
	     }
		
		cout << endl;
	}
	cout << endl;

	int B[3][3];
	for (int i = 0; i < 3; i++) {
		for (int j = 0; j < 3; j++) {
			B[j][i] = A[i][j];
		}
	}
		for (int  i = 0; i < 3; i++)
		{
			for (int j = 0; j < 3; j++)
			{
				cout << " " << B[i][j];
			}
			cout << endl;
		}


}
